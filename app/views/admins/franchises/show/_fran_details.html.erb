<div class = "card">
  <form>
    <div class = "card-body card-background-padgett">
      <div class = 'form-row col-12'>
        <!-- REGION -->
        <div class="form-group form-row col-6">
          <label class = "col-sm-6 col-md-4 col-lg-4 col-form-label label-bold">
            Region
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=Region.get_desc(franchise.region)%>
            </div>
          </div>
        </div>

        <!-- FRANCHISE -->
        <div class="form-group form-row col-6">
          <label class = 'col-sm-6 col-md-4 col-lg-4 col-form-label  label-bold'>
            Franchise
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=franchise.franchise_number%>
            </div>
          </div>
        </div>
      </div>

      <div class = 'form-row col-12'>
        <!-- OFFICE -->
        <div class="form-group form-row col-6">
          <label class = "col-sm-6 col-md-4 col-lg-4 col-form-label  label-bold">
            Office
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=franchise.office%>
            </div>
          </div>
        </div>

        <!-- FIRM ID -->
        <div class="form-group form-row col-6">
          <label class = 'col-sm-6 col-md-4 col-lg-4 col-form-label  label-bold'>
            Firm ID
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=franchise.firm_id%>
            </div>
          </div>
        </div>
      </div>
      
      <div class = 'form-row col-12'>
        <!-- LASTNAME -->
        <div class="form-group form-row col-6">
          <label class = "col-sm-6 col-md-4 col-lg-4 col-form-label  label-bold">
            Last Name
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=franchise.lastname%>
            </div>
          </div>
        </div>

        <!-- FIRSTNAME -->
        <div class="form-group form-row col-6">
          <label class = 'col-sm-6 col-md-4 col-lg-4 col-form-label  label-bold'>
            First Name
          </label>
          <div class = "col-sm-5 col-md-5 col-lg-8">
            <div class= 'form-control-plaintext'>
              <%=franchise.firstname%>
            </div>
          </div>
        </div>
      </div>

      <div class = 'form-row col-12'>
